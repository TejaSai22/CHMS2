# Git LFS tracking for large files
*.npy filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.keras filter=lfs diff=lfs merge=lfs -text
*.h5 filter=lfs diff=lfs merge=lfs -text
*.joblib filter=lfs diff=lfs merge=lfs -text
*.geojson filter=lfs diff=lfs merge=lfs -text

# Track large data files in data directories
data/**/*.npy filter=lfs diff=lfs merge=lfs -text
data/**/*.geojson filter=lfs diff=lfs merge=lfs -text
data/**/*.tif filter=lfs diff=lfs merge=lfs -text
data/**/*.tiff filter=lfs diff=lfs merge=lfs -text

# Track model files in outputs
outputs/**/models/*.h5 filter=lfs diff=lfs merge=lfs -text
outputs/**/models/*.pkl filter=lfs diff=lfs merge=lfs -text
outputs/**/models/*.keras filter=lfs diff=lfs merge=lfs -text
outputs/**/models/*.joblib filter=lfs diff=lfs merge=lfs -text
